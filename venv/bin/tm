#!/Users/jhardy/projects/python-projects/tm/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'tm','console_scripts','tm'
__requires__ = 'tm'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('tm', 'console_scripts', 'tm')()
    )
